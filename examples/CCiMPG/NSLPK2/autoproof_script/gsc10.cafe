-- sdm1
	:apply(tc)

	:def csb11 = :ctf [R#Rand \in ur(S#Sys) .]

	:apply(csb11)

	:imp [inv10] by {C:Cipher1 <- C@Cipher1 ;}

	:apply (rd)

