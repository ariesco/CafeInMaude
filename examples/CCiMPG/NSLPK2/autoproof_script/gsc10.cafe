	:apply(csb21)

	:def csb22 = :ctf [m1?(M#Msg) .]

	:apply(csb22)

	:def csb23 = :ctf [m2?(M0#Msg) .]

	:apply(csb23)

	:def csb24 = :ctf {eq creator(M#Msg) = P#Prin .}

	:apply(csb24)

	:def csb25 = :ctf {eq sender(M#Msg) = P#Prin .}

	:apply(csb25)

	:def csb26 = :ctf {eq receiver(M#Msg) = sender(M0#Msg) .}

	:apply(csb26)

	:def csb27 = :ctf {eq key(cipher1(M#Msg)) = sender(M0#Msg) .}

	:apply(csb27)

	:def csb28 = :ctf {eq nonce(cipher1(M#Msg)) = n(P#Prin,sender(M0#Msg),R#Rand) .}

	:apply(csb28)

	:def csb29 = :ctf {eq principal(cipher1(M#Msg)) = P#Prin .}

	:apply(csb29)

	:def csb30 = :ctf {eq receiver(M0#Msg) = P#Prin .}

	:apply(csb30)

	:def csb31 = :ctf {eq key(cipher2(M0#Msg)) = P#Prin .}

	:apply(csb31)

	:def csb32 = :ctf {eq nonce1(cipher2(M0#Msg)) = n(P#Prin,sender(M0#Msg),R#Rand) .}

	:apply(csb32)

	:def csb33 = :ctf {eq principal(cipher2(M0#Msg)) = sender(M0#Msg) .}

	:apply(csb33)

