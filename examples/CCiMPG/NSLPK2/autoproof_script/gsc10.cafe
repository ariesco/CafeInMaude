-- sdm2
	:apply(tc)

	:def csb14 = :ctf [M#Msg \in nw(S#Sys) .]

	:apply(csb14)

	:def csb15 = :ctf [m1?(M#Msg) .]

	:apply(csb15)

	:def csb16 = :ctf {eq receiver(M#Msg) = P#Prin .}

	:apply(csb16)

	:def csb17 = :ctf {eq key(cipher1(M#Msg)) = P#Prin .}

	:apply(csb17)

	:def csb18 = :ctf {eq principal(cipher1(M#Msg)) = sender(M#Msg) .}

	:apply(csb18)

	:def csb19 = :ctf [R#Rand \in ur(S#Sys) .]

	:apply(csb19)

	:imp [inv10] by {C:Cipher1 <- C@Cipher1 ;}

