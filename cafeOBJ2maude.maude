      VDS' := opDeclSetFromQidList(QIL) /\
      TOTF := renameOnTheFly(T, VDS') /\
      TOTF' := renameOnTheFly(T', VDS') /\
      TOTF'' := behavioralEq(renameOnTheFly(cafeEqAtS2maudeEqAts(T''), VDS')) /\
      PDR := parseDecl('ceq_=_if_.['bubble[TOTF], 'bubble[TOTF'], 'bubble[TOTF'']], PU, U, VDS) .

