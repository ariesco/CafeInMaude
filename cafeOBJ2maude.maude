  ceq createSearchEqsFromSorts(Srt ; SS) =
               createSearchEqsFromSorts(SS)
               eq '_==>_[CXU, CYU] = '_=`(_`,_`)=>*_[CXU, 's_['0.Zero], '*.NzNat*, CYU]
                                                 [metadata("built-in in CafeOBJ")] .
               eq '_==>1_[CXU, CYU] = '_=`(_`,_`)=>+_[CXU, 's_['0.Zero], '*.NzNat*, CYU]
                                                 [metadata("built-in in CafeOBJ")] .
               eq '_==>*_[CXU, CYU] = '_=`(_`,_`)=>*_[CXU, '*.NzNat*, '*.NzNat*, CYU]
                                                 [metadata("built-in in CafeOBJ")] .
               eq '_==>!_[CXU, CYU] = '_=`(_`,_`)=>!_[CXU, '*.NzNat*, '*.NzNat*, CYU]
