                                                 [metadata("built-in in CafeOBJ")] .
               eq '_==>1_withStateEq_[CXU, CYU, PRED] =
                        '_=`(_`,_`)=>+_withStateEq_[CXU, 's_['0.Zero], '*.NzNat*, CYU, PRED]
                                                 [metadata("built-in in CafeOBJ")] .
               eq '_==>*_withStateEq_[CXU, CYU, PRED] =
