 var  M : Module .
 vars S S' : Sort .
 vars SS SS' SS'' : SortSet .
 var  TyL : TypeList .
 var  K : Kind .
 var  AtS : AttrSet .
 var  ODS : OpDeclSet .
 var  MAS : MembAxSet .
 var  RS : RuleSet .
 var  Rl : Rule .
