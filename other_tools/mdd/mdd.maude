  op classSet : ClassDeclSet -> SortSet .

  eq classSet((class S:Sort | ADS:AttrDeclSet .) CDS:ClassDeclSet)
    = (S:Sort ; classSet(CDS:ClassDeclSet)) .
  eq classSet(none) = none .
