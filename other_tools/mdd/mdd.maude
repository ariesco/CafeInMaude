                            printList(M, TL, 1) '\n
   if T =>1 {TL} := getContents(A, NL) /\
      TL =/= empty .
  ceq printNode(A, NL, M) = '\n '\b 'Did 'you 'expect '\o
                            add-space(metaPrettyPrint(M, T, printOptions))
                            '\b 'to 'be 'final? '\o '\n
   if T =>1 {empty} := getContents(A, NL) .
