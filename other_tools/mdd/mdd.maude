  eq getFlatModuleNeg(MN,
       db(< MN ; DT ; U ; U' ; noModule ; VDS ; MNS ; VES > MIS,
          MNS', VIS, VES', MNS'', MNS3, MNS4, QIL))
    = unitError('\r 'Error: '\o
                'Module header2QidList(MN) 'not 'compiled. '\n) .
  eq getFlatModuleNeg(MN,
      db(< MN ; U ; U' ; U'' ; noModule ; VDS ; MNS ; VES > MIS,
