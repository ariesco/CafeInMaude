 eq quitConditions(M) = M [owise] .

 op quitConditions : RuleSet -> RuleSet .
 eq quitConditions(crl T => T' if C [AtS] . RS) = rl T => T' [nonexec AtS] . quitConditions(RS) .
 eq quitConditions(RS) = RS [owise] .

