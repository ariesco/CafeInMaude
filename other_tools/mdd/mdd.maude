fmod INDEX-UNIVERSAL-TESTING is
 pr MODULE-TRANSFORMATION-FOR-NARROWING .
 pr INDEX-PAIR .
 pr NAT-SET .

 vars M M' : Module .
 var  S : Sort .
 var  Q : Qid .
 vars T T' T'' T1 T2 : Term .
 vars TL TL' : TermList .
 var  IL : ImportList .
 var  SS : SortSet .
