\begin{verbatim}
%)

  crl [in] :
      [QIL, < O : X@Database | input : nilTermList, Atts >, QIL']
   => [nil,
       < O : X@Database | input : getTerm(metaParse(DD-GRAMMAR, QIL, '@Input@)), Atts >,
       QIL']
   if QIL =/= nil /\
      metaParse(DD-GRAMMAR, QIL, '@Input@) : ResultPair .

