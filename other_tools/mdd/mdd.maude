  eq rew-correct?(Q) = '\b 'Is 'this 'rewrite ' '`( 'associated 'with 'the 'rule '\o Q '\b '`) '
                       'correct? '\o '\n .

  op trans-rew-correct? : -> QidList .
  eq trans-rew-correct? = '\b 'Is 'this 'rewrite 'correct? '\o '\n .

