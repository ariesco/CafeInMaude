     COV1 := (COV & neededCovCM(fullLabeling(M), CM, COV)) int qidset2coverage(QS) /\
     COV2 := COV' int qidset2coverage(QS) /\
     CP := cp(COV1, COV2) /\
     {NM, CP'} := sysCovNarr(M'', CP, CIL, empty, V, MAS, CM) .
