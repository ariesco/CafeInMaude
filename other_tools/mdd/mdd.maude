                   currentFunction : Q, AtS >
   if M := if compiledModule(ME, DB)
           then getFlatModule(ME, DB)
           else getFlatModule(modExp(evalModExp(ME, DB)), database(evalModExp(ME, DB)))
           fi /\
      << QIL, TermT' >> := generateIsabelTestCases(M, Q, TB, SEED, TermT) .

