  ceq delete(eq T = T' [AtS] . EqS, QS) = delete(EqS, QS)
   if containsEquality(T') .
  ceq delete(ceq T = T' if C [AtS] . EqS, QS) = delete(EqS, QS)
   if containsEqualityCond(C) or containsEquality(T') .
