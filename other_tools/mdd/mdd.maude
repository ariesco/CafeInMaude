 op up2kind : Module Term -> Term .
 eq up2kind(M, Q[TL]) = Q[up2kind*(M, TL)] .
 eq up2kind(M, CONST) = CONST .
 ceq up2kind(M, V) = qid(string(Q) + ":" + string(K))
  if Q := getName(V) /\
